B = P.Toggle({
    Text = "Auto Equip Best",
    Callback = function(bool)
            getgenv().AutoEquipBest = bool
    print('Auto Tap Is', bool)
    spawn(function()
        while AutoEquipBest == true do
            game:GetService("ReplicatedStorage").Remotes.EquipBest:InvokeServer()
            wait(1)
        end
    end)
    end,
    Enabled = true
})

B = P.Button({
    Text = "Equip Best",
    Callback = function()
        game:GetService("ReplicatedStorage").Remotes.EquipBest:InvokeServer()    
